{
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "frontMatter.panel.openOnSupportedFile": false,
  "vue.server.includeLanguages": ["vue", "markdown"],
  "[json][jsonc][css][html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue][javascript][typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
