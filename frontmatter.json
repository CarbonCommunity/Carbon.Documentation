{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "string",
          "default": "doc"
        },
        {
          "title": "editLink",
          "name": "editLink",
          "type": "boolean",
          "default": true
        },
        {
          "title": "footer",
          "name": "footer",
          "type": "boolean",
          "default": true
        },
        {
          "title": "lastUpdated",
          "name": "lastUpdated",
          "type": "boolean",
          "default": true
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "docs",
      "path": "[[workspace]]/docs"
    },
    {
      "title": "development",
      "path": "[[workspace]]/docs/development"
    }
  ],
  "frontMatter.preview.host": "http://localhost:5173",
  "frontMatter.framework.startCommand": "npx vitepress dev docs",
  "frontMatter.website.host": "http://localhost:5173",
  "frontMatter.content.publicFolder": "[[workspace]]/docs/public/",
  "frontMatter.framework.id": "other"
}