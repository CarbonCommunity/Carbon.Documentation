{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "string",
          "default": "doc"
        },
        {
          "title": "editLink",
          "name": "editLink",
          "type": "boolean",
          "default": true
        },
        {
          "title": "footer",
          "name": "footer",
          "type": "boolean",
          "default": true
        },
        {
          "title": "lastUpdated",
          "name": "lastUpdated",
          "type": "boolean",
          "default": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "header",
          "name": "header",
          "type": "image"
        },
        {
          "title": "logo",
          "name": "logo",
          "type": "image"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "sidebar",
          "name": "sidebar",
          "type": "boolean"
        },
        {
          "title": "fmContentType",
          "name": "fmContentType",
          "type": "string"
        },
        {
          "title": "category",
          "name": "category",
          "type": "categories"
        },
        {
          "title": "published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "hidden",
          "name": "hidden",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "docs",
      "path": "[[workspace]]/docs"
    },
    {
      "title": "prefabs",
      "path": "[[workspace]]/docs/references/prefabs"
    },
    {
      "title": "loot",
      "path": "[[workspace]]/docs/references/loot"
    },
    {
      "title": "items",
      "path": "[[workspace]]/docs/references/items"
    },
    {
      "title": "hooks",
      "path": "[[workspace]]/docs/references/hooks"
    },
    {
      "title": "entities",
      "path": "[[workspace]]/docs/references/entities"
    },
    {
      "title": "blueprints",
      "path": "[[workspace]]/docs/references/blueprints"
    }
  ],
  "frontMatter.preview.host": "http://localhost:5173",
  "frontMatter.framework.startCommand": "npx vitepress dev docs",
  "frontMatter.website.host": "http://localhost:5173",
  "frontMatter.content.publicFolder": "[[workspace]]/docs/public/",
  "frontMatter.framework.id": "other"
}